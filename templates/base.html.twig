<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {#<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.9.0/fullcalendar.min.css"/>#}
            <link rel="stylesheet" href="{{ asset('css/fullcalendar/fullcalendar.min.css') }}" />
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" />
        {% endblock %}
    </head>
    <body>
        {% block body %}

        {% endblock %}

        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.3.1.min.js"
                    integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
                    crossorigin="anonymous">
            </script>
            <script type="text/javascript" src="{{ asset('js/fullcalendar/lib/moment.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/fullcalendar/fullcalendar.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/fullcalendar/locale-all.js') }}"></script>
        {% endblock %}
    </body>
</html>
